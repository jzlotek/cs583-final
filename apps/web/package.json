{
  "name": "SITD_Site",
  "version": "1.0.0",
  "description": "Simple web app to interact with the \"Learning to See in the Dark\" paper",
  "main": "index.js",
  "author": "Joseph Hines",
  "license": "MIT",
  "scripts": {
    "dev": "rm -rf .cache dist && cross-env NODE_ENV=development parcel public/index.html",
    "watch": "chokidar src/**/* src/* public/**/* public/* -c 'yarn build' --initial",
    "build": "cross-env NODE_ENV=production parcel build public/index.html --public-url / -d ../../static"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [">1%", "last 4 versions", "Firefox ESR", "not ie < 9"],
        "flexbox": "no-2009"
      }
    }
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.2",
    "cross-env": "^5.2.0"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^9.0.0",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
